.img{
  background-image:url('data:image/png;base64,');
  width:95%;
  height: 200rpx;
  background-size: cover;
  position: relative;
  margin: 20rpx auto;
}
/* 右半部,分 */
.hot,.imit,.imitTo{
  margin-left: 50rpx;
}
.hot{
  font-weight: 700;
  font-size: 32rpx;
  display: block;
  box-sizing: border-box;
  width: 60%;
  /* background: red; */
  position: absolute;
  top: 30rpx;
}
.imit{
   position: absolute;
  top: 100rpx;
   color: #888888;
  font-size: 25rpx;
}
.imitTo{
   position: absolute;
  top: 150rpx;
  color: #888888;
  font-size: 25rpx;
}
.reduce{
  margin-top: 5%;
  margin-left: 70%;
  color: white;
  width: 200rpx;
  height: 100rpx;
  position: absolute;
}
.onehundred{
  margin-top: 10rpx;
  font-size: 54rpx;
  text-align: left;
  font-weight: bold;
  margin-left: -2%;
