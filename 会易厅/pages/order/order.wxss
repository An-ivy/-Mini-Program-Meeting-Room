  background: white;
  margin: 25rpx 20rpx;
  border-radius: 20rpx;
  color: #fe8b7d;

}
/* 入住时间和价钱 */
.leave{
  display: flex;
  flex-direction: column;
  /* align-items: center;*/
  width: 60%;
  height: 150rpx;
  /* justify-content: center; */
  justify-content: space-around;
  margin-left: 28rpx;
  font-size: 30rpx;
}
/*价钱 */
.pary{
  width: 50%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-right: 30rpx;
}
/* 入住信息 */
.information{
  display: flex;
  background: white;
  margin: 20rpx 20rpx;
  border-radius: 20rpx;
  flex-direction: column;
}
.information_name{
  display: flex;
  justify-content: space-between;
  
}
.information_boder{
  border-bottom: 1px solid #fe8b7d;
  width: 95%;
  margin: 0 auto;
}
.information_titile{
  margin-left: 10rpx;
  padding:20rpx 0;
  font-weight: 500;
  color: #262626;
  font-size: 30rpx;
}
.Contacts{
  width: 100rpx;
  margin-right: 10rpx;
  margin: 15rpx 0;
  font-size: 25rpx;
  background: #fe8b7d;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  border-radius: 20rpx;
}
/* 姓名部分 */
.userName{
  /* border-bottom: 1px solid #868686; */
  width: 95%;
  margin: 0 auto;
  font-size: 26rpx;
  padding-left: 20rpx;
}
.userNamed{
  width: 95%;
  margin: 0 auto;
  font-size: 26rpx;
  padding-left: 20rpx;
}
.names{
  display: flex;
  padding: 15rpx 0;
}
.user{
  width: 40%;
  font-size: 26rpx;
  margin: auto 0;
}
/* 平台优惠 */
.discount{
  display: flex;
  justify-content: space-between;
  background: white;
  margin: 20rpx 20rpx;
  border-radius: 20rpx;
  height: 100rpx;
  align-items: center;
}
.discount_Y{
  margin-left: 20rpx;
  font-size: 25rpx;
  color: #464646;
  line-height: 100rpx;
}
.juan{
  font-size: 25rpx;
  margin-right: 20rpx;
  line-height: 100rpx;
}
.icon{
  color: #464646;
  margin: auto 5rpx;
  text-align: center;
}
.icon image{
  margin-top: 2.5%;
  position: absolute;
}
/* 入住条款 */
.clause{
  display: flex;
  justify-content: space-between;
  margin: 20rpx 20rpx;
  border-radius: 20rpx;
  align-items: center;
  height: 100rpx;
  margin-left: 4%;
}
.checkbox{
  font-size:25rpx;
}
.checkbox text{
  color: #fe8b7d;
  border-bottom: 1px solid #fe8b7d;
}
.switch{
  font-size: 25rpx;
}
/* 确认订单 */
.box{
  position: fixed;
  bottom: 0;
}
.confirmation{
  width: 750rpx;
  justify-content: space-between;
  background: white;
  border-radius: 20rpx;
  height: 120rpx;
  align-items: center; 
  line-height:120rpx;
  position: relative;
}
.morder{
  width:50%; 
  margin-left: 20rpx;
  color: #fe8b7d;
  float: left;
}
/* 确认按钮 */
.cc{
  width: 30%;
  /* font-weight: normal; */
  background: #fe8b7d;
  color: white;
  margin-right: 20rpx;
  position: absolute;
  right: 0%;
  bottom: 12%;
  font-size: 34rpx
}

/*模态框*/
.modals{
  position:fixed; 
  z-index: 999; 
  top:0; 
  left: 0; 
  right:0; 
  bottom: 0;
}
.modals-cancel{
  position:absolute; 
  z-index:1000; 
  top:0; 
  left: 0; 
  right:0; 
  bottom: 0; 
  background-color: rgba(0,0,0,.5);
}
.bottom-dialog-body{
  position:absolute; 
  z-index:10001; 
  bottom:0; 
  left:0; 
  right:0; 
  padding:30rpx; 
  height:500rpx; 
  background-color: #fff;
  width: 100%;
  font-size: 30rpx;
  color: #fe8b7d;
}
.bottom-dialog-body label{
  margin-left: 40%;
  margin-top: 8%;
}
.bottom-dialog-body image{
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin-right: 10%;
}
/*动画前初始位置*/
.bottom-pos{
  -webkit-transform:translateY(100%);transform:translateY(100%);
}
.modalscontent{
  width: 90%;
  background-color: #f0f0f0;
  margin-top: 4%;
  margin-left: 1%;
  display: flex;
  flex-direction: column;
  height: 370rpx;
}
.background{
  background-image:url('data:image/png;base64,');
  width:650rpx;
  height: 200rpx;
  background-size: 650rpx 200rpx;
  position: relative;
  margin: 20rpx auto;
  display: flex;
  flex-direction: row;
  align-items: center;
}
/* 右半部,分 */
.hot,.imit,.imitTo{
  margin-left: 50rpx;
}
.hot{
  font-weight: 700;
  font-size: 32rpx;
  display: block;
  box-sizing: border-box;
  width: 60%;
  /* background: red; */
  position: absolute;
  top: 30rpx;
}
.left{
  width: 35%;
  height: 90%;
}
.imit{
   position: absolute;
  top: 100rpx;
   color: #888888;
  font-size: 25rpx;
}
.imitTo{
   position: absolute;
  top: 150rpx;
  color: #888888;
  font-size: 25rpx;
}
#check{
  width: 100rpx;
  height: 40rpx;
  margin-left: 8%;
}
/* 满一千减 */
.reduce{
  margin-left: 70%;
  color: white;
  width: 150rpx;
  height: 100rpx;
  position: absolute;
}
.onehundred{
  margin-top: 20rpx;
  font-size: 54rpx;
  text-align: left;
  font-weight: bold;
  margin-left: -5%;
  height: 60rpx;
  width: 100rpx;
  position: absolute;
}
.bottom-dialog-body button{
  width: 80%;
  color: #fe8b7d;
  border: solid #fe8b7d;
  border-radius: 50rpx;
  height: 70rpx;
  font-size: 28rpx;
  line-height: 65rpx;
  margin-left: 2.5%;
  background-color: transparent;
  position: fixed;
  bottom: 10rpx;
}
/* 联系人模态框 */
/* 覆盖button样式 */
button.form_button{
  background-color:transparent;
  padding:0;
  margin:0;
  display:inline;
  position:static;
  border:0;
  padding-left:0;
  padding-right:0;
  border-radius:0;
  /* font-size:0rpx; */
  color:transparent;
}
button.form_button::after{
  content:'';
  width:0;
  height:0;
  -webkit-transform:scale(1);
  transform:scale(1);
  display:none;
  background-color:transparent;
}
 
.tip-content-dialog{
  position: fixed;
  display: flex;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color:white;
  z-index: 99999;
  flex-direction: column;
}
 
.tip-content-dialog .tip-dialog-view{
  width: 80%;
  margin: auto;
  border-radius: 25rpx;
  vertical-align: middle;
  animation: tanchu 400ms ease-in;
  /* overflow: hidden; */
  padding: 20rpx; 
}
 
.tip-content-dialog .btn{
  background: #f2f7fa;
}

.tip-content-dialog .dialogClose{
  position: absolute;
  right:20rpx;
  top: 10rpx;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
scroll-view{
